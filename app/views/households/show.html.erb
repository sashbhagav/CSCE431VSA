<style type="text/css">
   body {background-image: url(/assets/Event.png); background-size:1920px 950px;}
   #container{width:1920px; margin-right:auto; margin-left:auto}
  .event_show{padding-left: 120px; padding-top:100px; font-size:55px}
  .event_form{padding-left: 75px; padding-top:120px; font-size:25px}
  .form-buttons{padding-left: 275px; padding-top:50px;}
</style>
<body>
<div id = "container">
  <div class= "event_form" summary="event form">
      <br />
<p id="notice"><%= notice %></p>

<p>
  <strong>First:</strong>
  <%= @household.first %>
</p>

<p>
  <strong>Last:</strong>
  <%= @household.last %>
</p>

<p>
  <strong>Uin:</strong>
  <%= @household.UIN %>
</p>

<p>
  <strong>Family:</strong>
  <%= @household.family %>
</p>

<p>
  <strong>Email:</strong>
  <%= @household.email %>
</p>

<p>
  <strong>Phonenumber:</strong>
  <%= @household.phonenumber %>
</p>

<p>
  <strong>Classification:</strong>
  <%= @household.classification %>
</p>

<p>
  <strong>Major:</strong>
  <%= @household.major %>
</p>

<p>
  <strong>Points:</strong>
  <%= @household.points %>
</p>
</div>
  <div class = "form-buttons">
      <%= link_to 'Edit', edit_household_path(@household) %> |
      <%= link_to 'Back', households_path %>
  </div>
  </div>
</body>